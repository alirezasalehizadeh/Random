{
    "name": "alirezasalehizadeh/random",
    "description": "The PHP Random tool.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alirezasalehizadeh\\Random\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alirezasalehizadeh\\Random\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alireza Salehizadeh",
            "email": "alirezasalehizadehco@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "pestphp/pest": "2.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev"
}
